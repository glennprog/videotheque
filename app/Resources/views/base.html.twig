<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>{% block title %}GM waves center{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('frameworks/jquery/jquery-ui-1.12.1/jquery-ui.min.css') }}" />
            <link rel="stylesheet" href="{{ asset('frameworks/bootstrap/css/bootstrap.min.css') }}" />
            <link rel="stylesheet" href="{{ asset('frameworks/micron/css/micron.min.css') }}" />
            <link rel="stylesheet" href="{{ asset('application/css/application.css') }}" />
            <link rel="stylesheet" href="{{ asset('lib/gm/css/gm-ui-utils.css') }}" />
            <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.13/css/all.css" integrity="sha384-DNOHZ68U8hZfKXOrtjWvjxusGo9WQnrNx2sqG0tfsghAvtVlRW3tvkXWZh58N9jp" crossorigin="anonymous">
            <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
            <!--<link href="https://unpkg.com/webkul-micron@1.1.6/dist/css/micron.min.css" type="text/css" rel="stylesheet">-->        
        {% endblock %}
        {% block stylesheetsChild %}{% endblock %}
</head>
<body>
    {% block body %}
        <div id="app_wrapper" class="app-wrapper">
            <div id="app_bar_menu" class="app-barmenu">
                {% block app_bar_menu %}{% include 'GMVideothequeBundle::navigation.html.twig' %}{% endblock %}
            </div>
            <div id="app_side_menu" class="app-sidemenu">
                {% block app_side_menu %}{% endblock %}
            </div>
            <div id="app_content" class="container-fluid app-content">
                {% block flashbag %}
                    {% if app.request.hasPreviousSession %}
                        {% for type, messages in app.session.flashbag.all() %}
                            {% for message in messages %}
                                <div class="gm-alert gm-{{ type }}">
                                    <span class="gm-closebtn">&times;</span>  
                                    <strong>{{ type }}!</strong> {{ message }}
                                </div>
                            {% endfor %}
                        {% endfor %}
                    {% endif %}
                {% endblock %}
                {% block app_main_content %}{% endblock %}
            </div>
            <div id="app_footer" class="app-footer">
                {% block app_footer %}
                    <footer class="container margin-top-m gm-border-top">
                        <p class="float-right footer-text">Â© The sky is not the limit  Glenn Milingui, GM vawes center <(^__^)></p>
                    </footer>
                {% endblock %}
            </div>
        </div>
    {% endblock %}
    {% block javascripts %}
            <script src="{{ asset('frameworks/jquery/jquery-3.3.1.min.js') }}"></script>
            <script src="{{ asset('frameworks/jquery/jquery-ui-1.12.1/jquery-ui.min.js') }}"></script>
            <script type="text/javascript" src="http://code.highcharts.com/highcharts.js"></script>
            <script src="{{ asset('frameworks/bootstrap/js/bootstrap.min.js') }}"></script>
            <script src="{{ asset('frameworks/micron/js/micron.min.js') }}"></script>
            <script src="{{ asset('lib/gm/js/gm-utils.js') }}"></script>
            <script src="{{ asset('lib/gm/js/gm-ui-paginator.js') }}"></script>
            <script src="{{ asset('lib/gm/js/gm-ui-searchEngine.js') }}"></script>
            <script src="{{ asset('lib/gm/js/min/gm-ui-sort.min.js') }}"></script>
            <script src="{{ asset('lib/gm/js/min/gm-ui-utils.js') }}"></script>
            <script src="{{ asset('application/js/application.js') }}"></script>
            <!--<script src="https://unpkg.com/webkul-micron@1.1.6/dist/script/micron.min.js" type="text/javascript"></script>-->
    {% endblock %}
    {% block javascriptsChild %}{% endblock %}
</body>
</html>